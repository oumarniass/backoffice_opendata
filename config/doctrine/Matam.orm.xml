<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Matam" table="matam">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="departement" type="string" column="Departement" length="13" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="collectivite" type="string" column="Collectivite" length="24" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lieudevote" type="string" column="Lieudevote" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="bureauDeVotes" type="integer" column="Bureau de votes" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nombreElecteur" type="integer" column="Nombre electeur" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
